.row
  %h2.page-header
    TeamSeeds
    %span{class: "label label-primary"}
      = link_to "Ny TeamSeed", new_competition_team_seed_path(@competition),{style: "color:white;"}
  HUSK AT DET ER 0-INDEKSERT FRA TOPPEN
  %table.table.table-striped.table-hover
    %thead
      %th Lagnavn
      %th Posisjon i bracket
    %tbody
      - @team_seeds.each do |t|
        %tr
          %td=t.team_name
          %td=t.seed
          %td=link_to 'Endre', edit_competition_team_seed_path(@competition, t)
          %td=link_to 'x', competition_team_seed_path(@competition, t), method: :delete, data: {confirm: "Du sikker, bror?"}
